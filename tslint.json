{
    "extends": [
        "tslint:latest"
    ],
    "rules": {
        "array-type": [
            true,
            "array"
        ],
        "arrow-parens": false,
        "member-access": false,
        "member-ordering": [
            false
        ],
        "no-angle-bracket-type-assertion": true,
        "no-bitwise": false,
        "no-duplicate-variable": true,
        "no-namespace": false,
        "no-unused-variable": true,
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "prefer-const": true,
        "quotemark": [
            true,
            "single",
            "jsx-double",
            "avoid-escape"
        ],
        "radix": false,
        "semicolon": [
            true,
            "never"
        ]
    }
}
